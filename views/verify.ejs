<% include _header%>
<div class="container jumbotron">
  <div class="row">
    <div class="col-xs-12">
      <h1>Please verify your email with link sent to your email</h1>
      <form action="/verify/" method="POST">
        <div class="form-group">
          <input type="text" name="data" class="form-control" id="token" value="<%= JSON.stringify(data, null, ' ') %>"  hidden>
        </div>
      </form>
    </div>
  </div>
</div>
<% include _footer%>
